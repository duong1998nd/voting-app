<%- include('header') %>

    <div class="container">
        <h2>Danh mục</h2>
        <a href="/create" class="btn btn-sm btn-primary">Thêm mới danh mục</a>
        <hr>
        <table class="table table-hover">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Name</th>
                    <th>Status</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                <% this.userService.findAll().forEach((item, key)=> { %>
                    <tr>
                        <td>
                            <%= item.id %>
                        </td>
                        <td>
                            <%= item.name %>
                        </td>
                       
                        
                    </tr>
                    <% }) %>
            </tbody>
        </table>

    </div>

    <%- include('footer') %>